        function FiyatGetir(id, brk) {

            $.ajax({
                type: "GET",
                "datatype": "json",
                url: '/Servis/StokBilgiYeni?id=' + id + '&Brk=' + brk,
                success: function(gt) {
                    if (gt.success) {

                        if (gt.kodbulundu) {
                            $("#Barkod").val(gt.data.Barkod);
                        } else {

                            $("#StokKodu").select2("val", gt.data.ID);
                        }

                        $("#Birim").val("Adet");
                        $("#BirimFiyati").val(Number(gt.data.SatishFiyat).toFixed(2));
                        $("#KDV").val(gt.data.KDV);
                        $("#GarantiSuresi").val(gt.data.GarantiS);
                        Topla();


                    }
                }

            });


        }

