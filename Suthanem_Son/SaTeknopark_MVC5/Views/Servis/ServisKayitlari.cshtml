                icon: "warning",
                dangerMode: true,
                showCancelButton: true,
                confirmButtonColor: "#DD6B55",
                cancelButtonText: "Hayır",
                confirmButtonText: "Evet",
                closeOnCancel: true,
                closeOnConfirm: true,

                preConfirm: function() {
                    $.ajax({
                        type: "POST",
                        url: '@Url.Action("DeleteServis", "Servis")/' + id,
                        success: function(data) {
                            if (data.success) {
                                oTable.ajax.reload();
                                Temizle();
                            } else {
