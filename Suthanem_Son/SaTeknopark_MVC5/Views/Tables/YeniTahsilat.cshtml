    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/limonte-sweetalert2/7.2.0/sweetalert2.all.min.js"></script>
    <script src="~/Scripts/mask.js"></script>
    <script src="~/Scripts/musteri.js"></script>
    <link href="https://printjs-4de6.kxcdn.com/print.min.css" rel="stylesheet" />
    <script src="https://printjs-4de6.kxcdn.com/print.min.js"></script>
    <script src="~/Scripts/Fonksiyonlar.js"></script>
    <script type="text/javascript">

        function Kapat() {

            history.back();
        }

        function setTwoNumberDecimal(event) {
            this.value = parseFloat(this.value).toFixed(2);
        }

        $("#veri").on("submit", function (event) {
            event.preventDefault();
            var grubu = $("#Grubu").val();
            var Turu = $("#Turu").val();

            var Ksonuc = "T";
            var gelenH = document.getElementById("odemeturu_2").checked;
            var kredik = document.getElementById("odemeturu_4").checked;
            var nakitG = document.getElementById("odemeturu_0").checked;
            if (Boolean(nakitG) == true) Ksonuc = "T";

            if (Boolean(gelenH) == true) Ksonuc = "G";

            if (Boolean(kredik) == true) Ksonuc = "KKT";

            var ozelkodıd = $("#KodID").val();
            if (ozelkodıd == "") {
                ozelkodıd = -1        
            }



            if (grubu != "Teknik Servis Kullanıcısı") {


                var nakitO = document.getElementById("odemeturu_1").checked;
                var gidenH = document.getElementById("odemeturu_3").checked;
                if (Boolean(nakitO) == true) Ksonuc = "O";
                if (Boolean(gidenH) == true) Ksonuc = "H";

            }
            var obj = {
                IslemTipi: Ksonuc,
                IslemNo: $('#IslemNo').val(),
                id: $("#cID").val(),
                CariID: $("#CariID").val(),
                Aciklama: $("#Aciklama").val(),
                Tutar: $("#Tutar").val(),
                ThisKasaID: 0,
                ThisBankaID: 0,
                Parabirimi: $("#ParaBirimi").val(),
                durum: $("#durum").val(),
                Personel: $("#Personel").val(),
                OzelKodID: ozelkodıd,

            };

            if ($("#ThisKasaID").val() != null) {
                obj.ThisKasaID = $("#ThisKasaID").val();
            }
            if ($("#ThisBankaID").val() != null) {
                obj.ThisBankaID = $("#ThisBankaID").val();
            }
            var cariid = $("#CariID").val();
            if ($("#Tutar").val() != "0.00") {
                if (cariid != "") {

                    $.ajax({
                        type: "POST",
                        url: '/Tables/YeniTahsilat',
                        data: obj,
                        dataType: "json",

                        success: function (gelenDeg) {
                            if (gelenDeg.sonuc == "0")
                                swal("Uyarı", gelenDeg.Message, "warning");
                            else if (gelenDeg.sonuc == "3")
                                swal("Hata", gelenDeg.Message, "error");
                            else if (gelenDeg.sonuc == "4")
                                swal("Hata", gelenDeg.Message, "error");
                            else if (gelenDeg.sonuc == "1") {

                                swal({
                                    title: "Başarılı!",
                                    text: gelenDeg.Message,
                                    type: "success"
                                }).then(function () {
                                    //RaporGoster(gelenDeg.cid, 'T');
                                    var url = '@Url.Action("FinansHareketleri", "Finans")';

                                    if (grubu == "Teknik Servis Kullanıcısı") {
                                        url = '@Url.Action("Index","ServisList")';
                                    }
                                    window.location.href = url;
                                    temizle();
                                });
                            }
                        },
                        error: function () {
                            swal("Hata", "Kayıt Eklenirken Hata Oluştu!", "error");
                        }

                    });
                } else {
                    swal("HATA", "Lütfen Cari Seçiniz.", "warning");
                }
            } else {
                swal("UYARI", "Lütfen Tutar Giriniz.", "warning");
            }
        });

        function temizle() {

            $("#Aciklama").val("");
            $("#Tutar").val("0");

        }

        $(document).ready(function () {


            $(".digit").keypress(function (e) {
                if (e.which != 8 && e.which != 0 && e.which != 44 && (e.which < 48 || e.which > 57)) {
                    $("#errormsg").html("Digits Only").show().fadeOut("slow");
                    return false;
                }
            });


            $(".select2_demo_1").select2();
            $(".select2_demo_2").select2();
            $(".select2_demo_3").select2({
                placeholder: "Seçiniz",
                allowClear: true
            });


            $(".KasaDiv").hide('fast');
            $(".BankaDiv").hide('fast');
            $('input[type="radio"]').click(function () {
                if ($(this).attr("value") == "nakitT" || $(this).attr("value") == "nakitO") {
                    $(".KasaDiv").show('fast');
                    $(".BankaDiv").hide('slow');
                }
                if ($(this).attr("value") == "GelenH" || $(this).attr("value") == "GidenH") {
                    $(".KasaDiv").hide('slow');
                    $(".BankaDiv").show('fast');

                }
                if ($(this).attr("value") == "kredikarti") {
                    $(".KasaDiv").hide('slow');
                    $(".BankaDiv").show('fast');

                }
            });

            //$('input[type="radio"]').trigger('click');  // trigger the event

        });


    </script>
}
