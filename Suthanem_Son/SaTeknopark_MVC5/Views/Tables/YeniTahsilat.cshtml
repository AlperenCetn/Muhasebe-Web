                                        <div class="form-group">
                                            @Html.Hidden("durum", 0, new { @id = "durum" })
                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div class="col-md-3 px-1">
                                        <div class="form-group">
                                            @Html.Hidden("durum", -1, new { @id = "durum" })
                                        </div>
                                    </div>
                                }

                                <div class="col-md-3 px-1">

                                    <div class="form-group">
                                        @Html.Hidden("Grubu", @Session["Grubu"].ToString())
                                    </div>
                                </div>
                                <div class="col-md-3 px-1">

                                    <div class="form-group">
                                        @Html.Hidden("Turu", AyarMetot.ProTerminal)
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="offset-4 col-8">
                                        <button type="button" onclick="Kapat()" class="btn btn-default">Vazgeç</button>
                                        <button type="submit" class="btn btn-primary">Kaydet</button>


                                    </div>
                                </div>
                            </form>


                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


</div>
